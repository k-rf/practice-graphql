{
  "name": "practice_graphql_backend",
  "version": "1.2.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "check:lint": "eslint \"{src,test}/**\" --fix --cache --ext .tsx,.ts,.js",
    "check:format": "prettier . --write",
    "check:type": "tsc --project tsconfig.json --pretty --noEmit",
    "check:all": "npm-run-all -s check:type check:format check:lint",
    "test": "jest --runInBand",
    "test:cov": "jest --runInBand --coverage=true",
    "test:watch": "jest --runInBand --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.js --runInBand"
  },
  "lint-staged": {
    "*.{tsx,ts,js}": [
      "bash -c 'yarn check:type'",
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.0",
    "@nestjs/common": "^9.0.11",
    "@nestjs/core": "^9.0.11",
    "@nestjs/graphql": "^10.1.1",
    "@nestjs/mercurius": "^10.1.0",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/platform-fastify": "^9.0.11",
    "@nestjs/typeorm": "^9.0.1",
    "apollo-server": "^3.10.2",
    "apollo-server-express": "^3.10.2",
    "apollo-server-fastify": "^3.10.2",
    "dataloader": "^2.1.0",
    "fastify": "^4.6.0",
    "graphql": "^16.6.0",
    "mercurius": "^11.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "typeorm": "^0.3.10",
    "uuid": "^9.0.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.3",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.0.11",
    "@tsd/typescript": "^4.8.3",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.0.2",
    "@types/node": "^18.7.18",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "dotenv-cli": "^6.0.0",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-runner-tsd": "^4.0.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.2.4",
    "ts-jest": "29.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3"
  }
}
